<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.io.jappka.controller.ChartController">
   <children>
      <Button layoutX="267.0" layoutY="333.0" mnemonicParsing="false" onAction="#testStuff" prefHeight="25.0" prefWidth="74.0" text="Cancel">
         <font>
            <Font size="15.0" />
         </font></Button>
      <BarChart fx:id="barChart" layoutX="40.0" layoutY="14.0" prefHeight="308.0" prefWidth="505.0">
          <xAxis>
              <CategoryAxis side="BOTTOM" fx:id="xAxis" />
          </xAxis>
          <yAxis>
              <NumberAxis fx:id="yAxis" side="LEFT" />
          </yAxis>
      </BarChart>
   </children>
</AnchorPane>
