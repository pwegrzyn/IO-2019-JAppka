<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="506.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.edu.agh.io.jappka.controller.AddAppController">
    <children>
        <Button alignment="CENTER" layoutX="26.0" layoutY="447.0" mnemonicParsing="false" onAction="#handleBackButton" text="Cancel">
         <font>
            <Font size="15.0" />
         </font></Button>
        <Button alignment="CENTER" layoutX="189.0" layoutY="447.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="31.0" prefWidth="85.0" text="Add app">
         <font>
            <Font size="15.0" />
         </font></Button>
        <Button layoutX="104.0" layoutY="447.0" mnemonicParsing="false" onAction="#handleRefreshButton" text="Refresh">
         <font>
            <Font size="15.0" />
         </font></Button>
        <ListView fx:id="listView" layoutX="26.0" layoutY="79.0" onKeyPressed="#handleSelectApp" onMouseClicked="#handleOnClick" prefHeight="357.0" prefWidth="248.0" />
        <TextField fx:id="field" layoutX="26.0" layoutY="23.0" onKeyPressed="#handleOnTextChange" prefHeight="43.0" prefWidth="248.0" promptText="App name" />
    </children>
</AnchorPane>
